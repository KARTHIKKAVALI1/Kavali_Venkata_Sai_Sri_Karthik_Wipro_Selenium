Here are the total test cases of the eBay website :
In this code, all the test cases are covered, and many of the test cases are covered in one test case only.

package Testing_package;

import org.testng.annotations.Test;
import org.testng.annotations.BeforeTest;

import java.time.Duration;
import java.util.ArrayList;

import org.openqa.selenium.By;
import org.openqa.selenium.Keys;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.chrome.ChromeDriver;
import org.testng.Assert;
import org.testng.annotations.AfterTest;

public class TotalEbay {
	WebDriver driver;
	@BeforeTest//1(primary condition)
	  public void beforeTest() throws InterruptedException {
		  driver=new ChromeDriver();
		  driver.get("https://signin.ebay.com/ws/eBayISAPI.dll?SignIn&sgfl=gh&ru=https%3A%2F%2Fwww.ebay.com%2F");
			 WebElement search=driver.findElement(By.id("userid"));
			  search.sendKeys("saikarthik573@gmail.com");
			  Thread.sleep(3000);
			  search.sendKeys(Keys.ENTER);
			  driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(20));
			  WebElement search1=driver.findElement(By.id("pass"));
			  search1.sendKeys("Kavali9@");
			  search1.sendKeys(Keys.ENTER);
			  driver.manage().window().maximize();
			  Thread.sleep(5000); 
	  }
	
	@Test(priority = 1)
	public void search() { // this is default priority first it executes and then our priority 1,2,.......
		  WebElement search=driver.findElement(By.id("gh-ac"));
		  search.sendKeys("Watch");
		  search.sendKeys(Keys.ENTER);
		  
		  search = driver.findElement(By.id("gh-ac"));
		  search.clear();
		  search.sendKeys("Watches under 5000");
		  search.sendKeys(Keys.ENTER);
	}
	
	@Test(priority = 2)
	public void multipleSearch() throws InterruptedException {
		// both test cases multiple test case and checking functionality 2,3
		driver.get("https://www.ebay.com");
		driver.findElement(By.id("gh-ac")).sendKeys(Keys.ENTER);
	    Thread.sleep(4000);

	    
	    ArrayList<Integer> linkIndexes = new ArrayList<>();
	    for (int i = 1; i <= 18; i++) { 
	        linkIndexes.add(i);
	    }
	    for (int index : linkIndexes) {
	        WebElement link = driver.findElement(
	            By.xpath("//*[@id='wrapper']/div[1]/div/div/div[2]/div[1]/ul/li[" + index + "]/a")
	        );
	        link.sendKeys(Keys.ENTER);
	        Thread.sleep(1000); 
	    }
	}
	
	 @Test(priority = 3) //4
	  public void addCart() throws InterruptedException {
		  driver.get("https://www.ebay.com/itm/386230990003");
		  Thread.sleep(5000);
		  driver.findElement(By.xpath("//*[@id=\"atcBtn_btn_1\"]")).sendKeys(Keys.ENTER);
		  Thread.sleep(5000);
		  
	  }
	@Test(priority = 5)
	public void delCart() throws InterruptedException{
		Thread.sleep(5000);
		driver.findElement(By.xpath("//a[contains(@href,'cart.ebay.com')]")).click();
		driver.findElement(By.cssSelector("button.number-input__delete.icon-btn.icon-btn--small.icon-btn--transparent")).click();
		Thread.sleep(3000);
	}
	@Test(priority = 4)
	public void changeData() throws InterruptedException {
		//  //*[@id="account-settings-link-PI"]
		driver.get("https://accountsettings.ebay.com/uas");
		driver.findElement(By.xpath("//*[@id=\"account-settings-link-PI\"]")).sendKeys(Keys.ENTER);
		driver.findElement(By.xpath("//*[@id=\"individual_personal_info_address_edit_button\"]")).sendKeys(Keys.ENTER);
		Thread.sleep(2000);
		driver.findElement(By.xpath("//*[@id=\"individual_username_edit_button\"]")).sendKeys(Keys.ENTER);
		
		WebElement search = driver.findElement(By.id("user_name"));
		search.sendKeys("Karthik"); // TestCase - 6
		driver.findElement(By.id("username_submit_edit_btn")).sendKeys(Keys.ENTER);
		
		search=driver.findElement(By.id("firstName"));
		search.clear();
		search.sendKeys("Kira");
		search=driver.findElement(By.id("lastName"));
		search.clear();
		search.sendKeys("Kun");
		// Test Case - 7
		search = driver.findElement(By.id("addressLine1"));
		search.clear();
		search.sendKeys("India");
		search = driver.findElement(By.id("address_edit_submit_button"));
		search.sendKeys(Keys.ENTER);
		Thread.sleep(3000);
	}
	@Test(priority = 6)
    public void logout() {
    		
    		driver.findElement(By.xpath("//*[@id=\"gh\"]/nav/div[1]/span[1]/div/button")).sendKeys(Keys.ENTER);
    		driver.findElement(By.xpath("//*[@id=\"s0-1-4-9-3[0]-0-9-dialog\"]/div/div/ul/li[3]/a")).sendKeys(Keys.ENTER);
    }
	@AfterTest//3
	  public void afterTest() { //8
		  Assert.assertTrue(true,"Search button is not working");
		  driver.close();
	  }
}

Here is the pom.xml file : 

<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
                             https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>TestNG_selinilum</groupId>
  <artifactId>TestNG_selinilum</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  
  <!-- https://mvnrepository.com/artifact/org.testng/testng -->
  <dependencies>
<dependency>
    <groupId>org.testng</groupId>
    <artifactId>testng</artifactId>
    <version>7.11.0</version>
    <scope>test</scope>
</dependency>
<!-- https://mvnrepository.com/artifact/org.apache.poi/poi -->
<dependency>
    <groupId>org.apache.poi</groupId>
    <artifactId>poi</artifactId>
    <version>5.4.1</version>
</dependency>
<!-- https://mvnrepository.com/artifact/org.apache.poi/poi-ooxml -->
<dependency>
    <groupId>org.apache.poi</groupId>
    <artifactId>poi-ooxml</artifactId>
    <version>5.4.1</version>
</dependency>
</dependencies>
</project>


